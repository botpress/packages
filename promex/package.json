    "express": "4.18.2",
    "nanoid": "5.0.6",
    "prom-client": "15.1.0"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "18.19.20",
    "axios": "1.6.7",
    "typescript": "5.3.3",
    "vite": "5.1.4",
    "vitest": "1.3.1"
  },
  "scripts": {
    "build": "tsc --build",
    "type:check": "tsc --noEmit",

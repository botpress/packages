{
  "name": "@bpinternal/genenv",
  "version": "0.0.4",
  "description": "Small CLI to generate typescript file exporting constants from environment variables.",
  "bin": {
    "genenv": "./bin.js"
  },
  "scripts": {
    "check:type": "tsc --noEmit",
    "dev": "ts-node -T ./src/index.ts",
    "build": "ts-node -T ./build.ts",
    "test": "vitest run"
  },
  "license": "MIT",
  "dependencies": {
    "@bpinternal/yargs-extra": "0.0.19",
    "dotenv": "^16.4.4"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "esbuild": "^0.25.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.4",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": "8.6.2"
  },
  "packageManager": "pnpm@8.6.2"
}

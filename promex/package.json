{
  "name": "@bpinternal/promex",
  "description": "Prometheus express initialization package for nodejs.",
  "version": "0.5.17",
  "author": "Botpress, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/botpress/packages",
    "directory": "promex"
  },
  "license": "MIT",
  "main": "./dist/index.js",

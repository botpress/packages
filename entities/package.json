{
  "name": "@bpinternal/entities",
  "version": "0.0.1",
  "description": "Botpress Entities Extractor",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm run build:web && tsc -p ./tsconfig.build.json",
    "build:node": "wasm-pack build --verbose --target nodejs --release",
    "build:web": "wasm-pack build --verbose --target web --release",
    "dev": "ts-node -T ./src/index.ts",
    "start": "node ./dist/index.js",
    "test": "jest --roots ./dist --testPathIgnorePatterns \\.util\\.test\\.js && cargo test"
  },
  "dependencies": {
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@bpinternal/log4bot": "^0.0.4",
    "@bpinternal/yargs-extra": "^0.0.3",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.1",
    "esbuild": "^0.19.4",
    "jest": "^26.6.3",
    "puppeteer": "^21.3.6",
    "typescript": "^5.0.4",
    "wasm-pack": "^0.12.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
